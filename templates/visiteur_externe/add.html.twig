{% extends 'template.html.twig' %}

{% block title %}Hello Employes
{% endblock title  %}

{% block body %}


	<div class="container">
		
		<h1>Nouveau visiteur</h1>
		{{ form_start(VisiteurForm) }}
		<label>Nom</label>
		{{ form_widget(VisiteurForm.nom)}}
		<label>Prenoms</label>
		{{ form_widget(VisiteurForm.prenoms)}}
		<label>Email</label>
		{{ form_widget(VisiteurForm.email)}}
		<label>Téléphone</label>
		{{form_widget(VisiteurForm.tel)}}
	</div>
{% endblock body %}
